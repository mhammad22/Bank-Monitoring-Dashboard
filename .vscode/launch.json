    "version": "0.2.0",
    "configurations": [{
            "name": "Python: Celery",
            "type":"python",
            "module": "celery",
            "request": "launch",
            "console": "integratedTerminal",
            "args": [
                "-A",
                "core",
                "worker",
                "-l",
                "info",
            ],
            "django": true,
        },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
                // "8777",
                // "test",
                // "apps.integrations.flinks.tests.test_flinks"
            ],
            "django": true
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}